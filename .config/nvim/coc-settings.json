{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ],
  "coc.preferences.formatOnSaveFiletypes": ["css", "html", "Markdown", "dart", "rust", "json", "sql", "elixir"],
  "emmet.includeLanguages": {"template":"html"},
  "flutter.provider.enableSnippet": true,
  "rust.show_hover_context": true,
  "rust-client.updateOnStartup": true
}
